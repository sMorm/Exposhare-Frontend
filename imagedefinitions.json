{
  "taskDefinition": {
    "family": "exposhare-client",
    "executionRoleArn": "arn:aws:iam::466336452057:role/ecsTaskExecutionRole",,
    "containerDefinitions": [
      {
        "name": "exposhare-client",
        "image": "466336452057.dkr.ecr.us-east-1.amazonaws.com/exposhare-frontend:latest",
        "cpu": 10,
        "portMappings": [
          {
            "protocol": "tcp",
            "containerPort": 300,
            "hostPort": 80
          }
        ],
        "memory": 500,
        "essential": true
      }
    ]
  }
}